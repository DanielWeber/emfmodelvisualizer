<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="dataimport.dsl.editor.ImpEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="imp"
            icon="icons/file.gif"
            id="dataimport.dsl.editor.ImpEditor"
            name="imp Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="dataimport.dsl.wizards.NewImpProjectWizard"
			icon="icons/sample.gif"
			id="dataimport.dsl.wizards.NewImpProjectWizard"
			name="imp Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="dataimport.dsl.wizards.NewImpFileWizard"
			icon="icons/file.gif"
			id="dataimport.dsl.wizards.NewImpFileWizard"
			name="imp Editor file">
		</wizard>
	</extension>
</plugin>
