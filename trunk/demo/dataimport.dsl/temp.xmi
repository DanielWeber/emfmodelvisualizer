<?xml version="1.0" encoding="ASCII"?>
<xtext:XtextFile xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xtext="http://www.openarchitectureware.org/xtext">
  <rules xsi:type="xtext:TypeRule" name="DataStructure">
    <content>
      <alternatives xsi:type="xtext:Keyword" value="data">
        <next xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
          <next xsi:type="xtext:Keyword" value="{">
            <next xsi:type="xtext:Group" cardinality="ANY">
              <next xsi:type="xtext:Keyword" value="}"/>
              <firstElement xsi:type="xtext:Alternatives">
                <alternatives xsi:type="xtext:Assignment" feature="attributes" operator="ADD">
                  <token xsi:type="xtext:RuleName" name="Attribute"/>
                </alternatives>
                <alternatives xsi:type="xtext:Assignment" feature="references" operator="ADD">
                  <token xsi:type="xtext:RuleName" name="Reference"/>
                </alternatives>
              </firstElement>
            </next>
          </next>
          <token xsi:type="xtext:RuleName" name="ID"/>
        </next>
      </alternatives>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Reference">
    <content>
      <alternatives xsi:type="xtext:Keyword" value="->">
        <next xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
          <next xsi:type="xtext:Keyword" value=":">
            <next xsi:type="xtext:Assignment" feature="type" operator="ASSIGN">
              <next xsi:type="xtext:Group" cardinality="OPTIONAL">
                <firstElement xsi:type="xtext:Alternatives">
                  <alternatives xsi:type="xtext:Assignment" feature="ismulti" operator="BOOLASSIGN">
                    <token xsi:type="xtext:Keyword" value="[]"/>
                  </alternatives>
                </firstElement>
              </next>
              <token xsi:type="xtext:CrossReference" type="DataStructure"/>
            </next>
          </next>
          <token xsi:type="xtext:RuleName" name="ID"/>
        </next>
      </alternatives>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="Attribute">
    <content>
      <alternatives xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
        <next xsi:type="xtext:Keyword" value=":">
          <next xsi:type="xtext:Assignment" feature="type" operator="ASSIGN">
            <token xsi:type="xtext:RuleName" name="ID"/>
          </next>
        </next>
        <token xsi:type="xtext:RuleName" name="ID"/>
      </alternatives>
    </content>
  </rules>
</xtext:XtextFile>
