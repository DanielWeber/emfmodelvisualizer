<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.openarchitectureware.vis.graphmm.editor.GraphmmEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="gmm"
            icon="icons/file.gif"
            id="org.openarchitectureware.vis.graphmm.editor.GraphmmEditor"
            name="graphmm Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.openarchitectureware.vis.graphmm.wizards.NewGraphmmProjectWizard"
			icon="icons/template.gif"
			id="org.openarchitectureware.vis.graphmm.wizards.NewGraphmmProjectWizard"
			name="graphmm Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.openarchitectureware.vis.graphmm.wizards.NewGraphmmFileWizard"
			icon="icons/file.gif"
			id="org.openarchitectureware.vis.graphmm.wizards.NewGraphmmFileWizard"
			name="graphmm Editor file">
		</wizard>
	</extension>
</plugin>
